@model HomeShowCourseStudentsViewModel

<h2>Students in @Model.CourseName</h2>

<table class="table table-hover table-responsive-md">
    <tr>
        <th>Student name</th>
    </tr>
    @foreach(var studentName in Model.StudentNames)
    {
        <tr>
            <td>@studentName</td>
        </tr>
    }

</table>