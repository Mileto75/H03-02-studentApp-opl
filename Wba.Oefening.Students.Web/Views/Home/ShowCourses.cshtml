@model List<HomeShowCoursesViewModel>

<h2>@ViewBag.PageTitle</h2>

<table class="table table-hover">
    <tr>
        <th>Course name</th>
    </tr>
    @foreach(var course in @Model)
    {
        <tr>
            <td><a href="courses/@course.Id/students">@course.Name</a></td>
        </tr>
    }

</table>

