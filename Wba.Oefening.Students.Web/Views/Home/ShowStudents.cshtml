@model HomeShowStudentsViewModel


<h2>Students</h2>
<table class="table table-hover table-responsive-md">
    <tr>
        <th>Studet name</th>
    </tr>
    @foreach(var student in Model.Students)
    {
        <tr>
            <td><a href="students/@student.StudentId">@student.StudentName</a></td>
        </tr>
    }
</table>

